{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
    "crons": [
    {
      "path": "/api/reels/fetch",
      "schedule": "45 19 * * *"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "GET",
        "POST",
        "DELETE"
      ]
    }
  ]
}
